game Pika [] !
	
	
	
	pika1 << INT
	pika2 << BOOLEAN
	pika3 << STRING
	pika4 << DOUBLE
	pika5 << CHAR
	
	pika1 <- 98324
	pika2 <- TRUE
	pika3 <- "hello world"
	pika4 <- 9.43
	pika5 <- 'c'
	
	mymethod METHOD_ONE INT [INT pika1, BOOLEAN pika2, STRING pika3] !
		
		var1 << INT
		var1 <- METHOD_ONE[var2, var3, var4]
		
		METHOD_ONE[var1]
		void_METHODTWO[]
		METHOD_ONE[var2, var3, var4]
		void_METHODTHREE[var1, var2, var3]
		
		
		jackieReturns var1
	!
	
	mymethod METHOD_TWO VOID [BOOLEAN var1, BOOLEAN var2, STRING var3] !
		var1 << INT
		var2 << INT
		var3 << CHAR
		
		var3 <- 'a'
		var1 <- 1 + 2
		var2 <- ((2/9) + (2*2)) 
		
		METHOD_ONE[var1]
		void_METHODTWO[]
		METHOD_ONE[var2, var3, var4]
		void_METHODTHREE[var1, var2, var3]
	!
!
